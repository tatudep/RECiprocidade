generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Ong {
  id    String @id @default(auto()) @map("_id") @db.ObjectId
  name  String
  email String @unique
  cause String
}

model Empresa {
  id     String @id @default(auto()) @map("_id") @db.ObjectId
  name   String
  email  String @unique
  sector String
}
